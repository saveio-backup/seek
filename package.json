{
  "name": "seek",
  "version": "1.0.2-7",
  "description": "seek about ONI",
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "scripts": {
    "i": "npm i",
    "oldi": "npm i --build-from-source --runtime=electron --target=5.0.6  --dist-url=https://atom.io/download/electron",
    "preload": "npx webpack --config ./.electron-vue/webpack.preload.config.js",
    "build": "npm run preload && node .electron-vue/build.js && electron-builder",
    "build:mac": "npm run preload && node .electron-vue/build.js && electron-builder --m",
    "build:win": "npm run preload && node .electron-vue/build.js && electron-builder --w --x64",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "npm run preload && node .electron-vue/dev-runner.js",
    "e2e": "npm run pack && mocha test/e2e",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src test",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src test",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "test": "npm run unit && npm run e2e",
    "unit": "karma start test/unit/karma.conf.js",
    "postinstall": "npm run lint:fix"
  },
  "build": {
    "productName": "Seeker",
    "artifactName": "${productName}_${version}.${ext}",
    "appId": "com.application.seeker",
    "directories": {
      "output": "build"
    },
    "extraFiles": [
      {
        "from": "resources/${os}",
        "to": "Resources/bin",
        "filter": [
          "**/*"
        ]
      }
    ],
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/seeker_logo_origin.png",
      "artifactName": "${productName}_Setup_${version}_alpha.${ext}"
    },
    "win": {
      "icon": "build/icons/seeker.png",
      "artifactName": "${productName}_Setup_${version}_alpha.${ext}"
    },
    "linux": {
      "icon": "build/icons"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true
    }
  },
  "dependencies": {
    "adm-zip": "^0.4.13",
    "app-root-dir": "^1.0.2",
    "axios": "^0.18.0",
    "cfonts": "^2.4.3",
    "chalk": "^2.4.2",
    "echarts": "^4.2.1",
    "electron-log": "^3.0.5",
    "element-ui": "^2.11.0",
    "highlight.js": "^9.15.10",
    "leveldown": "^5.3.0",
    "levelup": "^4.3.2",
    "marked": "^0.7.0",
    "mathjs": "^6.2.1",
    "node-uuid": "^1.4.8",
    "spark-md5": "^3.0.0",
    "uninstall": "0.0.0",
    "vue": "^2.6.10",
    "vue-electron": "^1.0.6",
    "vue-i18n": "^8.14.1",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-electron": "^1.0.0"
  },
  "devDependencies": {
    "ajv": "^6.5.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^2.1.2",
    "chai": "^4.1.2",
    "chalk": "^2.4.1",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^5.0.8",
    "electron-builder": "^21.2.0",
    "electron-debug": "^3.0.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-rebuild": "^1.8.6",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "inject-loader": "^4.0.1",
    "karma": "^2.0.2",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.2",
    "karma-electron": "^6.0.0",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^3.0.0",
    "mini-css-extract-plugin": "0.4.0",
    "mocha": "^5.2.0",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.11.0",
    "require-dir": "^1.0.0",
    "sass-loader": "^7.1.0",
    "spectron": "^3.8.0",
    "style-loader": "^0.21.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "url-loader": "^1.0.1",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.2.4",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.1.3"
  }
}
